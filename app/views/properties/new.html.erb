<html>
<h3> Nueva propiedad</h3>

<%=form_for @property do |f| %>
<div class="form-group">
    <%=f.label :nombre, 'Nombre'%>
    <%=f.text_field :nombre, class: 'form-control', placeholder: 'Ingrese el nombre de la propiedad'%>
  </div>
<div class="form-group">
    <%=f.label :descripcion, 'Descripcion'%>
    <%=f.text_field :descripcion, class: 'form-control', placeholder: 'Ingrese la descripcion de la propiedad'%>
  </div>
<div class="form-group">
    <%=f.label :lugar, 'Lugar'%>
    <%=f.text_field :lugar, class: 'form-control', placeholder: 'Ingrese el lugar donde se encuentra la propiedad'%>
  </div>
<% end %>
 </html> 